{
  "name": "gwilym",
  "type": "dark",
  "colors": {
    "editor.background": "#0a0a0a",
    "editor.foreground": "#f4f4f5",
    "editorLineNumber.foreground": "#71717a",
    "editorLineNumber.activeForeground": "#a1a1aa",
    "editorCursor.foreground": "#8b5cf6",
    "editor.selectionBackground": "#27272a",
    "editor.inactiveSelectionBackground": "#18181b"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#71717a", "fontStyle": "italic" }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#f59e0b" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#8b5cf6" }
    },
    {
      "scope": ["entity.name.function", "support.function", "variable.function"],
      "settings": { "foreground": "#a78bfa" }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": { "foreground": "#0ea5e9" }
    },
    {
      "scope": ["entity.name.type", "support.type"],
      "settings": { "foreground": "#10b981" }
    },
    {
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#a1a1aa" }
    }
  ]
}
